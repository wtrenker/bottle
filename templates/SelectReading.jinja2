{% extends "Base.jinja2" %}
{% block content %}
      <form action = "/select" method="post"  enctype="multipart/form-data">
            <input type="hidden" name="sessionID" value="{{ sessionID }}">
            <div>
               <h2>List of Held Dates</h2>
               <h2><label for="dateList"></label></h2>
                <h2><select id="dateList" name="selectedDate"></h2>
                    {% for heldDate in heldReadingDates %}
                        <h2><option name="{{ heldDate[0] }}">{{ heldDate[1] }}</option></h2><br>
                    {% endfor %}
                </select>
               <br>
               <h2><input type="submit" value="Select Date"></h2><br>
            </div>
        </form>
    <h2><a href="/admin">Admin</a></h2>
{% endblock %}
