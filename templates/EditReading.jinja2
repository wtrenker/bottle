{% extends "Base.jinja2" %}
{% block content %}
<form action="/update" method="post" enctype="multipart/form-data">
    <input type="hidden" value="{{ sessionID }}" name="sessionID">
    <h2>Date: <input type="text" readonly value="{{ form.date }}" name="date"></h2>
    <h2>AM Reading: <input type="text" readonly value="{{ form.am }}" name="am"></h2>
    <h2>PM Reading: <input type="text" value="{{ form.pm }}" name="pm"></h2>
    <h2>Annotation: <input type="text" size="50" value="{{ form.comment }}" name="comment"></h2>
    <h2><input type="submit" value="Save Changes"></h2><br>
</form>
    <h2><a href="/admin">Cancel</a></h2>
    <dbFileName {{ dbFileName }}>
{% endblock %}
